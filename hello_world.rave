import "raylib"

void main {
    InitWindow(800, 450, "Hello, Rave!");

    while(WindowShouldClose() == 0) {
        BeginDrawing();
            ClearBackground(Color(255, 255, 255, 255));
            DrawText("Congrats! You created your first window!", 190, 200, 20, Color(200, 200, 200, 255));
        EndDrawing();
    }
}